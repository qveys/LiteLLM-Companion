      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus-vps"
          },
          "expr": "sum(increase(ai_browser_domain_active_duration_seconds_total{host_name=~\"$host\", browser_name=~\"$browser\", ai_category=~\"$category\", usage_source=~\"$source\"}[1d]))",
